/*Table structure for table `tbl_roles` */

CREATE TABLE `tbl_roles` (
  `roleId` tinyint(4) NOT NULL AUTO_INCREMENT COMMENT 'role id',
  `role` varchar(50) NOT NULL COMMENT 'role text',
  `type` int(1) DEFAULT NULL,
  `buy_fee` varchar(5) DEFAULT NULL,
  `point_acceptRate` varchar(5) DEFAULT NULL,
  `sending_times` varchar(10) DEFAULT NULL,
  `coupon_type` int(1) DEFAULT NULL,
  `sending_inul` varchar(5) DEFAULT NULL,
  `coupon_gold` varchar(10) DEFAULT NULL,
  `use` int(1) NOT NULL DEFAULT '1',
  `updated_time` datetime DEFAULT NULL,
  `recomman` varchar(10) DEFAULT NULL,
  `level` varchar(5) DEFAULT NULL,
  `sending_times1` varchar(10) DEFAULT NULL,
  `address_rate` text,
  PRIMARY KEY (`roleId`)
) ENGINE=MyISAM AUTO_INCREMENT=11 DEFAULT CHARSET=utf8;

