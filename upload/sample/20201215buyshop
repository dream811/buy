/*Table structure for table `tbl_purchasedproduct` */

CREATE TABLE `tbl_purchasedproduct` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `delivery_id` int(11) DEFAULT NULL,
  `trackingNumber` varchar(50) DEFAULT NULL,
  `order_number` varchar(50) DEFAULT NULL,
  `category` int(5) DEFAULT NULL,
  `productName` varchar(200) DEFAULT NULL,
  `unitPrice` varchar(10) DEFAULT NULL,
  `count` int(10) DEFAULT NULL,
  `size` text,
  `url` text,
  `image` text,
  `updated_date` datetime DEFAULT CURRENT_TIMESTAMP,
  `step` int(2) DEFAULT NULL,
  `trackingHeader` varchar(20) DEFAULT NULL,
  `parent_category` int(5) DEFAULT NULL,
  `color` varchar(100) DEFAULT NULL,
  `created_date` datetime DEFAULT NULL,
  `shop` int(11) DEFAULT NULL,
  `old_delivery_id` int(11) DEFAULT NULL,
  `serial` varchar(100) DEFAULT NULL,
  `tracking_time` datetime DEFAULT CURRENT_TIMESTAMP,
  `add_options` text,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=1525 DEFAULT CHARSET=utf8;

