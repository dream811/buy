  `id` int(11) NOT NULL AUTO_INCREMENT,
  `delivery_id` int(11) DEFAULT NULL,
  `all_amount` varchar(10) DEFAULT NULL,
  `payed_date` datetime DEFAULT NULL,
  `type` int(1) DEFAULT NULL,
  `amount` varchar(10) DEFAULT NULL,
  `payed_type` varchar(10) DEFAULT NULL,
  `pending` int(1) NOT NULL DEFAULT '0',
  `coupon` int(1) DEFAULT '0',
  `userId` int(11) DEFAULT NULL,
  `security` varchar(20) DEFAULT NULL,
  `pamount` varchar(10) DEFAULT NULL,
  `updated_date` datetime NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `content` varchar(50) DEFAULT NULL,
  `tt` int(1) NOT NULL DEFAULT '1',
  `by` int(1) DEFAULT NULL,
  `point` int(11) NOT NULL DEFAULT '0',
  `coupon_type` text,
  PRIMARY KEY (`id`,`updated_date`)
) ENGINE=InnoDB AUTO_INCREMENT=406 DEFAULT CHARSET=utf8;

