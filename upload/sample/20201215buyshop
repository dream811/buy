/*Table structure for table `tbl_mail` */

CREATE TABLE `tbl_mail` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `toId` int(11) DEFAULT NULL,
  `fromId` int(11) DEFAULT NULL,
  `title` varchar(50) DEFAULT NULL,
  `content` text,
  `type` int(2) DEFAULT NULL,
  `updated_date` datetime DEFAULT CURRENT_TIMESTAMP,
  `view` varchar(10) NOT NULL DEFAULT '0',
  `category` varchar(30) DEFAULT NULL,
  `mode` varchar(20) DEFAULT NULL,
  `security` int(1) DEFAULT NULL,
  `letter_l` int(1) NOT NULL DEFAULT '0',
  `file1` text,
  `file2` text,
  `file3` text,
  `anyone` int(1) DEFAULT '0',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=531 DEFAULT CHARSET=utf8;

/*Data for the table `tbl_mail` */

