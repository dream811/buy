/*Table structure for table `tbl_product_talk` */

CREATE TABLE `tbl_product_talk` (
  `id` int(10) NOT NULL AUTO_INCREMENT,
  `type` varchar(20) NOT NULL DEFAULT '' COMMENT '댓글타입:상품토크,상품후기 등',
  `pcode` varchar(30) DEFAULT NULL COMMENT '상품코드',
  `userId` varchar(50) DEFAULT NULL COMMENT '등록 회원',
  `writer` varchar(50) NOT NULL DEFAULT '' COMMENT '작성자 이름',
  `intype` enum('normal','admin','company') NOT NULL DEFAULT 'normal' COMMENT '등록 회원 유형 normal:일반회원, admin:운영자, company:해당입점업체',
  `title` varchar(255) DEFAULT NULL COMMENT '제목',
  `content` mediumtext COMMENT '댓글내용',
  `rdate` datetime DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '등록일',
  `depth` tinyint(2) DEFAULT '1' COMMENT '1:부모 , 2:자식글 구분',
  `relation` int(10) DEFAULT '0' COMMENT '부모글 m_uid, 자식글이 아니면 0',
  `eval_point` int(11) NOT NULL DEFAULT '0' COMMENT '평점',
  `img` varchar(200) DEFAULT NULL COMMENT '상품평가 이미지',
  PRIMARY KEY (`id`),
  KEY `id` (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=120 DEFAULT CHARSET=utf8 COMMENT='상품 토크,후기,문의 ';

