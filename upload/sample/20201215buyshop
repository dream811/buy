/*Table structure for table `tbl_users` */

CREATE TABLE `tbl_users` (
  `userId` int(11) NOT NULL AUTO_INCREMENT,
  `email` varchar(128) NOT NULL COMMENT 'login email',
  `password` text NOT NULL COMMENT 'hashed login password',
  `name` varchar(128) DEFAULT NULL COMMENT 'full name of user',
  `mobile` varchar(20) DEFAULT NULL,
  `roleId` tinyint(4) NOT NULL,
  `isDeleted` tinyint(4) NOT NULL DEFAULT '0',
  `createdBy` int(11) DEFAULT NULL,
  `createdDtm` date NOT NULL,
  `updatedBy` int(11) DEFAULT NULL,
  `updatedDtm` datetime DEFAULT NULL,
  `aid` varchar(30) DEFAULT NULL,
  `nickname` varchar(30) DEFAULT NULL,
  `birthday` varchar(100) DEFAULT NULL,
  `postNum` varchar(10) DEFAULT NULL,
  `address` varchar(100) DEFAULT NULL,
  `detail_address` varchar(100) DEFAULT NULL,
  `telephone` varchar(15) DEFAULT NULL,
  `smsRecevice` int(1) DEFAULT NULL,
  `deposit` varchar(10) NOT NULL DEFAULT '0',
  `loginId` varchar(30) DEFAULT NULL,
  `phone_verification` int(1) DEFAULT '0',
  `email_verification` int(1) DEFAULT '0',
  `emailRecevice` int(1) DEFAULT NULL,
  `point` varchar(10) NOT NULL DEFAULT '0',
  `sase` varchar(10) DEFAULT NULL,
  `log_num` int(10) NOT NULL DEFAULT '0',
  `log_date` date DEFAULT NULL,
  `complete_orders` int(11) NOT NULL DEFAULT '0',
  `session_id` varchar(100) DEFAULT NULL,
  PRIMARY KEY (`userId`)
) ENGINE=MyISAM AUTO_INCREMENT=98 DEFAULT CHARSET=utf8;

