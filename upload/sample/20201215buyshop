/*Table structure for table `tbl_coupon` */

CREATE TABLE `tbl_coupon` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `gold` varchar(10) DEFAULT NULL,
  `gold_type` int(1) DEFAULT NULL,
  `terms` varchar(30) DEFAULT NULL,
  `coupon_type` int(1) DEFAULT NULL,
  `use_terms` int(3) DEFAULT NULL,
  `use` int(1) DEFAULT NULL,
  `event_coupon` int(1) DEFAULT NULL,
  `send` int(1) DEFAULT '0',
  `event` int(1) DEFAULT NULL,
  `code` varchar(100) DEFAULT NULL,
  `created_date` datetime DEFAULT NULL,
  `is_deleted` int(1) NOT NULL DEFAULT '0',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=99 DEFAULT CHARSET=utf8;

/*Data for the table `tbl_coupon` */

