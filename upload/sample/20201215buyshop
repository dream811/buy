  `id` int(11) NOT NULL AUTO_INCREMENT,
  `step` int(2) DEFAULT NULL,
  `type` int(1) DEFAULT NULL,
  `name` varchar(20) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=45 DEFAULT CHARSET=utf8;

