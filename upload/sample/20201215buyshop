  `onebacs_package` varchar(10) NOT NULL DEFAULT '0',
  `customs_fee` varchar(10) NOT NULL DEFAULT '0',
  `sum_delivery` varchar(10) NOT NULL DEFAULT '0',
  `multi_package` varchar(10) NOT NULL DEFAULT '0',
  `save_fee` varchar(10) NOT NULL DEFAULT '0',
  `add_fee` varchar(10) NOT NULL DEFAULT '0',
  `wonsanji` varchar(10) NOT NULL DEFAULT '0',
  `multi_bacs` varchar(10) NOT NULL DEFAULT '0',
  `volumn_price` varchar(10) NOT NULL DEFAULT '0',
  `mountain_price` varchar(10) NOT NULL DEFAULT '0',
  `tax_fee` varchar(10) NOT NULL DEFAULT '0',
  `throw_price` varchar(10) NOT NULL DEFAULT '0',
  `quarantine` varchar(10) NOT NULL DEFAULT '0',
  `carton_price` varchar(10) NOT NULL DEFAULT '0',
  `gate_fee` varchar(10) NOT NULL DEFAULT '0',
  `return_fee` varchar(10) NOT NULL DEFAULT '0',
  `volunmn_rate` varchar(3) NOT NULL DEFAULT '0',
  `credit_fee` varchar(10) DEFAULT NULL,
  `type` int(2) DEFAULT NULL,
  `point` int(11) NOT NULL DEFAULT '0',
