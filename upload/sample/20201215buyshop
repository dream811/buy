/*Table structure for table `tbl_product_option` */

CREATE TABLE `tbl_product_option` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT '고유번호',
  `pcode` varchar(50) NOT NULL COMMENT '상품코드',
  `name` varchar(100) DEFAULT NULL COMMENT '옵션명',
  `count` int(10) NOT NULL DEFAULT '0' COMMENT '옵션 수량',
  `salecount` int(10) NOT NULL DEFAULT '0' COMMENT '옵션 판매량',
  `optionprice` int(10) DEFAULT '0' COMMENT '옵션가격',
  `supply` int(10) NOT NULL DEFAULT '0' COMMENT '옵션 공급가',
  `depth` tinyint(2) unsigned DEFAULT '1' COMMENT '옵션 차수 - 1,2,3차',
  `parent` varchar(200) DEFAULT NULL COMMENT '부모 uid 지정',
  `view` enum('Y','N') NOT NULL DEFAULT 'Y' COMMENT '옵션보이기',
  `sort` int(11) NOT NULL DEFAULT '1' COMMENT '옵션순서',
  PRIMARY KEY (`id`),
  KEY `po_pcode` (`pcode`),
  KEY `po_depth` (`depth`),
  KEY `po_parent` (`parent`),
  KEY `po_poptionname` (`name`),
  KEY `po_view` (`view`)
) ENGINE=MyISAM AUTO_INCREMENT=5195 DEFAULT CHARSET=utf8 COMMENT='상품옵션관리';

