/*Table structure for table `delivery` */

CREATE TABLE `delivery` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `place` int(1) DEFAULT NULL,
  `incoming_method` int(1) DEFAULT NULL,
  `billing_name` varchar(50) DEFAULT NULL,
  `billing_krname` varchar(50) DEFAULT NULL,
  `phone_number` varchar(20) DEFAULT NULL,
  `post_number` varchar(10) DEFAULT NULL,
  `address` varchar(100) DEFAULT NULL,
  `detail_address` varchar(100) DEFAULT NULL,
  `request_detail` varchar(100) DEFAULT NULL,
  `main_check` int(11) NOT NULL DEFAULT '-10',
  `detail_check` int(11) NOT NULL DEFAULT '-10',
  `shoemedical` int(11) NOT NULL DEFAULT '-10',
  `tackbae` int(11) NOT NULL DEFAULT '-10',
  `package_protect` int(11) NOT NULL DEFAULT '-10',
  `special_package` int(11) NOT NULL DEFAULT '-10',
  `onebacs_package` int(11) NOT NULL DEFAULT '-10',
  `ganyifee` int(11) NOT NULL DEFAULT '-10',
  `wonsanji` int(11) NOT NULL DEFAULT '-10',
  `multibacs` int(11) NOT NULL DEFAULT '-10',
  `volumn_weight` int(11) NOT NULL DEFAULT '-10',
  `mountain_fee` int(11) NOT NULL DEFAULT '-10',
  `logistics_request` varchar(30) DEFAULT NULL,
  `type` int(1) DEFAULT NULL,
  `state` int(2) DEFAULT NULL,
  `updated_date` datetime DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `userId` int(11) DEFAULT NULL,
  `payed` varchar(10) DEFAULT '0',
  `tracking_header` varchar(20) DEFAULT NULL,
  `person_num` int(2) DEFAULT NULL,
  `person_unique_content` varchar(20) DEFAULT NULL,
  `delivery_number` varchar(10) DEFAULT NULL,
  `get` varchar(10) DEFAULT NULL,
  `sending_price` varchar(10) DEFAULT '0',
  `payed_send` int(1) DEFAULT '0',
  `created_date` datetime DEFAULT NULL,
  `cutoms_fee` varchar(10) DEFAULT NULL,
  `real_weight` varchar(10) DEFAULT '0',
  `width` varchar(10) DEFAULT '0',
  `height` varchar(10) DEFAULT '0',
  `length` varchar(10) DEFAULT '0',
  `accept_rate` varchar(10) DEFAULT '100',
  `sum_delivery` int(1) NOT NULL DEFAULT '-10',
  `save_fee` int(1) NOT NULL DEFAULT '-10',
  `multipackage` int(11) DEFAULT '-10',
  `vlm_wt` varchar(10) NOT NULL DEFAULT '0',
  `mem_wt` varchar(10) DEFAULT NULL,
  `updated_date_price` datetime DEFAULT NULL,
  `payed_checked` int(1) DEFAULT '0',
  `memo` text,
  `tracking_number` varchar(20) DEFAULT NULL,
  `out_date` date DEFAULT NULL,
  `ordernum` varchar(100) DEFAULT NULL,
  `purchase_price` varchar(11) NOT NULL DEFAULT '0',
  `return_check` int(1) DEFAULT '0',
  `return_price` varchar(10) NOT NULL DEFAULT '0',
  `combine` int(1) DEFAULT '-1',
  `backs` int(2) NOT NULL DEFAULT '1',
  `pays` int(1) DEFAULT '0',
  `sends` varchar(11) DEFAULT NULL,
  `pur_fee` varchar(100) DEFAULT NULL,
  `cur_send` int(11) NOT NULL DEFAULT '0',
  `rfee` varchar(11) DEFAULT NULL,
  `shop` text,
  `complete_orders` int(11) NOT NULL DEFAULT '0',
  `return_file` text,
  `rid` varchar(50) DEFAULT NULL,
  `old_ordernum` varchar(100) DEFAULT NULL,
  `tracking_time` datetime DEFAULT CURRENT_TIMESTAMP,
  `add_options` text,
  `free_delivery` int(1) NOT NULL DEFAULT '0',
  `free_price` varchar(10) DEFAULT NULL,
  `options` text,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=712 DEFAULT CHARSET=utf8;

/*Data for the table `delivery` */

