/*Table structure for table `tbl_prodcucts` */

CREATE TABLE `tbl_prodcucts` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(20) DEFAULT NULL,
  `brand` varchar(20) DEFAULT NULL,
  `origin_price` varchar(10) DEFAULT NULL,
  `color` varchar(15) DEFAULT NULL,
  `image` text,
  `short_description` varchar(100) DEFAULT NULL,
  `bef_sales` varchar(10) DEFAULT NULL,
  `product_price` varchar(10) DEFAULT NULL,
  `sales_term` varchar(50) DEFAULT NULL,
  `size` varchar(10) DEFAULT NULL,
  `thumbnail` text,
  `use` int(1) DEFAULT '0',
  `detailed_desctiption` text,
  `updated_date` datetime DEFAULT NULL,
  `created_date` datetime DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=8 DEFAULT CHARSET=utf8;

