/*Table structure for table `tbl_popup` */

CREATE TABLE `tbl_popup` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `title` varchar(50) DEFAULT NULL,
  `content` text,
  `terms` varchar(30) DEFAULT NULL,
  `size_w` varchar(10) DEFAULT NULL,
  `size_h` varchar(10) DEFAULT NULL,
  `po_w` varchar(10) DEFAULT NULL,
  `po_h` varchar(10) DEFAULT NULL,
  `scroll_use` int(1) DEFAULT NULL,
  `use` int(1) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8;

