/*Table structure for table `banner` */

CREATE TABLE `banner` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `description` text,
  `image` text,
  `link` text,
  `use` int(1) DEFAULT NULL,
  `target` int(1) DEFAULT NULL,
  `title` varchar(50) DEFAULT NULL,
  `size_w` varchar(10) DEFAULT NULL,
  `size_h` varchar(10) DEFAULT NULL,
  `type` int(2) DEFAULT NULL,
  `content` text,
  `mt` int(11) NOT NULL DEFAULT '0',
  `mb` int(11) NOT NULL DEFAULT '0',
  `order` int(11) DEFAULT NULL,
  `image1` text,
  `image2` text,
  `header` int(1) NOT NULL DEFAULT '0',
  `footer` int(1) NOT NULL DEFAULT '0',
  `my` int(1) NOT NULL DEFAULT '0',
  `mobile` int(1) NOT NULL DEFAULT '0',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=169 DEFAULT CHARSET=utf8;

