  `id` int(11) NOT NULL AUTO_INCREMENT,
  `title` varchar(30) DEFAULT NULL,
  `size_w` varchar(10) DEFAULT NULL,
  `size_h` varchar(10) DEFAULT NULL,
  `image` text,
  `halin_price` varchar(10) DEFAULT NULL,
