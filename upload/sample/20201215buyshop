/*Table structure for table `tbl_category` */

CREATE TABLE `tbl_category` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(50) DEFAULT NULL,
  `parent` int(10) DEFAULT NULL,
  `general_customs` int(1) NOT NULL DEFAULT '1',
  `hs_code` varchar(20) DEFAULT NULL,
  `customs_price` varchar(10) DEFAULT NULL,
  `en_subject` varchar(50) DEFAULT NULL,
  `kr_subject` varchar(50) DEFAULT NULL,
  `chn_subject` varchar(10) DEFAULT NULL,
  `tariff_rate` varchar(10) DEFAULT NULL,
  `vat_rate` varchar(10) DEFAULT NULL,
  `use` int(1) NOT NULL DEFAULT '1',
  `food` int(1) NOT NULL DEFAULT '0',
  `updated_date` datetime DEFAULT NULL,
  `orders` int(10) NOT NULL DEFAULT '1',
  `image` text,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=530 DEFAULT CHARSET=utf8;

